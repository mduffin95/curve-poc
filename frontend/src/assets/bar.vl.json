{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "description": "A simple bar chart with embedded data.",
  "data": {
    "name": "table"
  },
  "mark": {
    "type": "line",
    "point": true
  },
  "encoding": {
    "x": {"timeUnit": "year", "field": "maturityDate", "type": "temporal"},
    "y": {"aggregate":"mean", "field": "yield", "type": "quantitative"}
  }
}
